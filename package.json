{
  "devDependencies": {
    "@faker-js/faker": "^10.0.0",
    "@trivago/prettier-plugin-sort-imports": "^5.2.2",
    "@types/node": "^24.3.0",
    "@types/pg": "^8.15.5",
    "allure-commandline": "^2.34.1",
    "allure-cypress": "^3.3.3",
    "cypress": "^14.5.4",
    "cypress-plugin-api": "^2.11.2",
    "dotenv": "^17.2.1",
    "husky": "^9.1.7",
    "lint-staged": "^16.2.5",
    "prettier": "^3.6.2",
    "ts-node": "^10.9.2",
    "typescript": "^5.9.2"
  },
  "dependencies": {
    "pg": "^8.16.3",
    "superstruct": "^2.0.2"
  },
  "scripts": {
    "prepare": "husky",
    "test": "cypress run",
    "test:allure": "cypress run --env allure=true",
    "allure:generate": "allure generate allure-results --clean",
    "allure:open": "allure open allure-report",
    "test:report": "npm run test:allure && npm run allure:generate && npm run allure:open",
    "format": "prettier --write ."
  },
  "lint-staged": {
    "*.{ts,js,json,md}": [
      "prettier --write"
    ]
  }
}
